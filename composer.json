{
    "name": "bobimicroweber/filament-dropdown-column",
    "description": "Use custom dropdowns on filament tables as column",
    "keywords": [
        "bobimicroweber",
        "laravel",
        "filament-dropdown-column"
    ],
    "homepage": "https://github.com/bobimicroweber/filament-dropdown-column",
    "support": {
        "issues": "https://github.com/bobimicroweber/filament-dropdown-column/issues",
        "source": "https://github.com/bobimicroweber/filament-dropdown-column"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bozhidar Slaveykov",
            "email": "bobi@microweber.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/tables": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BobiMicroweber\\FilamentDropdownColumn\\": "src/",
            "BobiMicroweber\\FilamentDropdownColumn\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BobiMicroweber\\FilamentDropdownColumn\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BobiMicroweber\\FilamentDropdownColumn\\FilamentDropdownColumnServiceProvider"
            ],
            "aliases": {
                "FilamentDropdownColumn": "BobiMicroweber\\FilamentDropdownColumn\\Facades\\FilamentDropdownColumn"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}